{
  "name": "livevue-sigil-highlighting",
  "displayName": "LiveVue sigil highlighting (~VUE)",
  "homepage": "https://hexdocs.pm/live_vue/readme.html",
  "description": "Syntax highlighting support for the (~VUE) sigil",
  "author": "Guilherme Ferreira",
  "publisher": "guilhermepsf",
  "version": "0.0.1",
  "icon": "images/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/GuilhermePSF/live-vue-sigil-highlighting"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "elixir",
    "vue",
    "heex",
    "sigil",
    "liveview",
    "live_vue",
    "~VUE"
  ],
  "contributes": {
    "grammars": [
      {
        "scopeName": "source.elixir.vue",
        "path": "./syntaxes/elixir-vue.json",
        "injectTo": [
          "source.elixir"
        ],
        "embeddedLanguages": {
          "text.html.vue": "vue"
        }
      }
    ]
  }
}
